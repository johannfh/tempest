gpf alias g 'echo g (alias) -> git && git' alias ga 'echo ga (alias) -> git add && git add' alias gc 'echo gc (alias) -> git commit -v && git commit -v' alias gd 'echo gd (alias) -> git diff && git diff' alias gp 'echo gp (alias) -> git push && git push' alias gpf 'echo gpf (alias) -> echo "git push --force-with-lease" && git push --force-with-lease && echo "git push --force-with-lease" && git push --force-with-lease' alias gs 'echo gs (alias) -> git status && git status' - git push --force-with-lease
